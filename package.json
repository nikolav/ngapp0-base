    "@tailwindcss/typography": "^0.5.19",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/lodash": "^4.17.21",
    "@types/md5": "^2.3.6",
    "@types/minimist": "^1.2.5",
    "@types/node": "^18.18.0",
    "autoprefixer": "^10.4.23",
