{
  "name": "app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "cp ./src/app/config/vars.env.development.ts ./src/app/config/vars.env.ts && cat ./src/app/assets/index.development.html > ./src/index.html && ng serve ",
    "build": "cp ./src/app/config/vars.env.production.ts ./src/app/config/vars.env.ts && cat ./src/app/assets/index.production.html > ./src/index.html && ng build ",
    "build:static": "ng build --output-mode=static",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:app": "node dist/app/server/server.mjs",
    "tinker": "npx tsx tinker.ts "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.17",
